<h1>Users</h1>
<table class="table table-border">
    <thead>
        <tr>
            <th>#</th>
            <th>Name</th>
            <th>Email</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(function(user, index) { %>
            <tr>
                <td><%= (index + 1) %></td>
                <td><%= user.name %></td>
                <td><%= user.email %></td>
                <td></td>
            </tr>
        <% }); %>
    </tbody>
</table>